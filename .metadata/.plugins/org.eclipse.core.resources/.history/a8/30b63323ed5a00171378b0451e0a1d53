        http://www.springframework.org/schema/context/spring-context.xsd
        http://www.springframework.org/schema/aop
        http://www.springframework.org/schema/aop/spring-aop.xsd">
	<bean id="classMethod" class="com.study.spring.method.ClassMethod"></bean>
	<bean id="aopBefore" class="com.study.spring.aop.ClassMethod.In2BeforAop"></bean>
	<aop:config>
		<aop:pointcut expression="* com.study.spring.method.ClassMethod.myMethod(*)" id="clsMeth"/>
		<aop:aspect>
			<aop:before method=""/>
		</aop:aspect>
	</aop:config>
